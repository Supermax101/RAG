    {
      "type": "text",
      "section": "- Steatosis",
      "line": 95,
      "preview": "- Avoid overfeeding CHO and calories (Use mixed fuel system including dextrose, amino acids and fat)"
    },
    {
      "type": "text",
      "section": "- Steatosis",
      "line": 96,
      "preview": "- Limit ILE infusions to $<2.5 \\mathrm{~g} / \\mathrm{kg} / \\mathrm{d}$"
    },
    {
      "type": "text",
      "section": "- Steatosis",
      "line": 97,
      "preview": "- Cholelithiasis"
    },
    {
      "type": "text",
      "section": "- Steatosis",
      "line": 98,
      "preview": "- Early oral or enteral nutrition to stimulate gallbladder contraction"
    },
    {
      "type": "text",
      "section": "- Steatosis",
      "line": 99,
      "preview": "- Reduce the load of PUFA and other Long chain fatty acids in PN"
    },
    {
      "type": "text",
      "section": "- Steatosis",
      "line": 100,
      "preview": "- Cholestasis"
    },
    {
      "type": "text",
      "section": "- Steatosis",
      "line": 101,
      "preview": "- Avoid long term PN if possible"
    },
    {
      "type": "text",
      "section": "- Steatosis",
      "line": 102,
      "preview": "- Early initiation of enteral nutrition to stimulate gallbladder contraction and reduce microbial ov"
    },
    {
      "type": "text",
      "section": "- Steatosis",
      "line": 103,
      "preview": "- Limit phytosterol infusions from ILE"
    },
